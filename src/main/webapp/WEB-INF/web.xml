<?xml version="1.0" encoding="utf-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
		<servlet-name>HandshakeOne</servlet-name>
		<servlet-class>com.horsefire.tiddly.appengine.dropbox.HandshakeOneServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HandshakeOne</servlet-name>
		<url-pattern>/handshake/one</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>HandshakeTwo</servlet-name>
		<servlet-class>com.horsefire.tiddly.appengine.dropbox.HandshakeTwoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HandshakeTwo</servlet-name>
		<url-pattern>/handshake/two</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Wiki</servlet-name>
		<servlet-class>com.horsefire.tiddly.appengine.WikiServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Wiki</servlet-name>
		<url-pattern>/wiki/*</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>